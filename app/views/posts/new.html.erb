<%= form_with(model: @post, local: true) do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <%= f.label :fee %><br>
  <%= f.text_field :fee %><br>
  <%= f.label :due_date %><br>
  <%= f.date_field :due_date %><br>
  <%= f.label :experience %><br>
  <%= f.text_field :experience %><br>
  <%= f.label :detail %><br>
  <%= f.text_field :detail %><br>
  <%= f.submit "投稿", class: "btn btn-primary" %><br>
<% end %>
